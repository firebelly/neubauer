{
  "require": {
    "carlcs/craft-uielementfields": "^1.0",
    "clubstudioltd/craft-asset-rev": "^6.0",
    "craftcms/cms": "3.7.36",
    "craftcms/feed-me": "4.4.2",
    "craftcms/mailgun": "^2.0",
    "craftcms/redactor": "^2.10",
    "cspoo/swiftmailer-mailgun-bundle": "^2.0",
    "doublesecretagency/craft-cpcss": "^2.4",
    "ether/seo": "^3.7",
    "guilty/craft-slugify": "^1.0",
    "mmikkel/cp-field-inspect": "1.2.6",
    "mmikkel/reasons": "^2.2",
    "nystudio107/craft-twigpack": "^1.2",
    "putyourlightson/craft-blitz": "^3.11",
    "putyourlightson/craft-sprig": "^1.11",
    "sebastianlenz/linkfield": "^1.0",
    "spacecatninja/imager-x": "^3.5",
    "studioespresso/craft-date-range": "2.5.0",
    "twig/extra-bundle": "^3.3",
    "twig/string-extra": "^3.3",
    "vaersaagod/matrixmate": "^1.3",
    "verbb/field-manager": "^2.2",
    "verbb/super-table": "^2.7",
    "vlucas/phpdotenv": "^5.4.0",
    "wrav/oembed": "^1.3"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "controlpanel\\": "modules/controlpanel/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.27"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
